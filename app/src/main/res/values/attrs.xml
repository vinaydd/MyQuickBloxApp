<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HorizontalCalendarView">
        <!-- Text color for all dates except selected Date.-->
        <attr name="textColorNormal" format="color" />
        <!-- Text color for selected Date. -->
        <attr name="textColorSelected" format="color" />
        <!-- Background color for selector bar. -->
        <attr name="selectorColor" format="color" />
        <!-- Background color for selected date item. -->
        <attr name="selectedDateBackground" format="color" />
    </declare-styleable>


    <declare-styleable name="ChipView">
        <attr name="chip_spacing" format="dimension" />
        <attr name="chip_line_spacing" format="dimension" />
        <attr name="chip_padding" format="dimension" />
        <attr name="chip_side_padding" format="dimension" />
        <attr name="chip_corner_radius" format="dimension" />
        <attr name="chip_background" format="reference" />
        <attr name="chip_background_selected" format="reference" />
        <attr name="chip_background_res" format="reference" />
    </declare-styleable>


    <declare-styleable name="RTCGlView">
        <attr name="mainMirror" format="boolean" />
        <attr name="secondMirror" format="boolean" />
        <attr name="mainCoords" format="reference" />
        <attr name="secondCoords" format="reference" />
    </declare-styleable>

    <declare-styleable name="SeekBarPreference">
        <attr name="min" format="integer" />
        <attr name="stepSize" format="integer" />
    </declare-styleable>



</resources>